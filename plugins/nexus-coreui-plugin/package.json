    "@nivo/core": "^0.79.0",
    "@nivo/line": "^0.79.0",
    "@sonatype/react-shared-components": "13.1.6",
    "@xstate/react": "1.3.4",
    "axios": "1.7.4",
    "classnames": "2.3.1",
    "luxon": "1.28.1",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swagger-ui-react": "4.17.1",
    "xstate": "4.16.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/plugin-proposal-export-default-from": "7.16.7",
