{
  "name": "nexus-coreui-plugin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all build-dev copy-dev",
    "build-all": "npm-run-all build-dev build-prod",
    "build-dev": "webpack --config webpack.dev.js",
    "build-prod": "webpack --config webpack.prod.js",
    "copy-dev": "cpy \"target/classes/static/nexus-coreui-bundle.debug.js\" \"target/classes/static/\" --rename=\"nexus-coreui-bundle.js\"",
    "test": "jest --config src/frontend/jest.config.js",
    "test-coverage": "jest --config src/frontend/jest.config.js --coverage",
    "test-watch": "jest --config src/frontend/jest.config.js --watch",
    "test-watch-debug": "node --inspect-brk node_modules/.bin/jest --config src/frontend/jest.config.js --runInBand --watchAll",
    "watch": "webpack --config webpack.dev.js  --watch"
  },
  "dependencies": {
    "@babel/runtime": "=7.6.0",
    "@fortawesome/fontawesome-svg-core": "=1.2.19",
    "@fortawesome/free-solid-svg-icons": "=5.9.0",
    "@xstate/react": "=1.0.0-rc.3",
    "axios": "=0.19.0",
    "classnames": "=2.2.6",
    "react": "=16.11.0",
    "react-dom": "=16.11.0",
    "xstate": "=4.7.5"
  },
  "devDependencies": {
    "@babel/core": "=7.4.5",
    "@babel/plugin-transform-runtime": "=7.6.0",
    "@babel/plugin-proposal-optional-chaining": "=7.6.0",
    "@babel/preset-env": "=7.4.5",
    "@babel/preset-react": "=7.0.0",
    "@testing-library/jest-dom": "=4.2.4",
    "@testing-library/react": "=9.4.0",
    "acorn": "=7.0.0",
    "babel-jest": "=24.8.0",
    "babel-loader": "=8.0.6",
    "babel-polyfill": "=6.26.0",
    "copy-modules-webpack-plugin": "=2.0.4",
    "cpy-cli": "=3.1.1",
    "css-loader": "=3.0.0",
    "file-loader": "=6.0.0",
    "jest": "=24.8.0",
    "jest-junit": "=7.0.0",
    "mini-css-extract-plugin": "=0.7.0",
    "nexus-ui-plugin": "0.1.0",
    "node-sass": "=4.12.0",
    "optimize-css-assets-webpack-plugin": "=5.0.3",
    "sass-loader": "=7.1.0",
    "terser-webpack-plugin": "=1.3.0",
