    "jest-when": "3.1.0",
    "mini-css-extract-plugin": "2.7.2",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "sass": "1.38.0",
    "sass-loader": "12.6.0",
    "terser-webpack-plugin": "5.3.1",
    "webpack": "5.94.0",
    "webpack-cli": "4.9.2",
